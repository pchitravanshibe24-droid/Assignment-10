#include <bits/stdc++.h>
using namespace std;

void countFrequency(const vector<int>& nums) {
    unordered_map<int, int> freq;    // number -> count

    for (int x : nums) {
        freq[x]++;                   // increase count
    }

    for (auto &p : freq) {
        cout << p.first << " -> " << p.second << " times\n";
    }
}

int main() {
    vector<int> nums = {2, 3, 2, 4, 3, 2};

    countFrequency(nums);

    return 0;
}
