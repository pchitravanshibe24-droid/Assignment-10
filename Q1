#include <bits/stdc++.h>
using namespace std;

bool containsDuplicate(const vector<int>& nums) {
    unordered_set<int> seen;        

    for (int x : nums) {
        if (seen.find(x) != seen.end()) {
            // already present â†’ duplicate
            return true;
        }
        seen.insert(x);
    }
    return false;
}

int main() {
    vector<int> nums = {1, 2, 3, 1};

    if (containsDuplicate(nums))
        cout << "true\n";
    else
        cout << "false\n";

    return 0;
}
